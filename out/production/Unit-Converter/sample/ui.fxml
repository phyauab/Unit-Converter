<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <ListView fx:id="lv_from" layoutX="50.0" layoutY="140.0" prefHeight="200.0" prefWidth="200.0" />
      <ListView fx:id="lv_to" layoutX="350.0" layoutY="140.0" prefHeight="200.0" prefWidth="200.0" />
      <TextField fx:id="tf_from" layoutX="50.0" layoutY="94.0" prefHeight="23.0" prefWidth="200.0" />
      <TextField fx:id="tf_to" layoutX="350.0" layoutY="94.0" prefHeight="23.0" prefWidth="200.0" />
      <Button fx:id="btn_convert" layoutX="250.0" layoutY="94.0" mnemonicParsing="false" onAction="#convert" prefHeight="23.0" prefWidth="100.0" text="Convert" />
      <Label fx:id="label_from" layoutX="125.0" layoutY="48.0" text="From">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="label_to" layoutX="438.0" layoutY="48.0" text="To">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
